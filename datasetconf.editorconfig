from sklearn.datasets import make_classification

# Создание искусственного датасета
X, y = make_classification(
    n_samples=1000,  # Общее количество образцов
    n_features=10,   # Количество признаков
    n_classes=2,     # Количество классов (бинарная классификация)
    random_state=42  # Зафиксируем seed для воспроизводимости
)


